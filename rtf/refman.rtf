  935      * format caract\'E8re : 1 column vide + 5 columns r\'E9ellement utilis\'E9es\par
  936      * + 2 columns vides */\par
  937     for( i=0,j=0; i<taille; i++,j++)\par
  938     \{\par
  939         if(message[i]==195)  // les lettres accentu\'E9es sont cod\'E9es sur deux octets  (195 167 pour \'E7)\par
  940         \{\par
  941             i++;\par
  942             k++;\par
  943         \}\par
  944     // row suivante \'E0 d\'E9commenter pour obtenir le code des caract\'E8res UTF8\par
  945     // std::cout << "code : " << (int)message[i] << std::endl;\par
  946         ConvertCharacterToPattern(message[i],chaine[j],colorText,colorBackground);\par
  947     \}\par
  948     taille = taille - k;\par
  949     nombreDecolumns=(taille)*8-2;\par
  950     k = 0;\par
  951     // Parcours de toutes les columns de tous les patterns qui compose\par
  952     // la chaine de caract\'E8res \'E0 afficher pour supprimer les columns vides sauf celle\par
  953     // qui s\'E9pare les patterns (caract\'E8res). + gestion du caract\'E8re espace.\par
  954     for( l=0;l<nombreDecolumns;l++)\par
  955     \{\par
  956         i=l/8;\par
  957         k=l%8;\par
  958 \par
  959 //      if(columnVide(k,chaine[i],colorBackground)) // Une column Vide avant chaque caract\'E8re \'E0 ne pas supprimer\par
  960         if(EmptyColumn(k,chaine[i],colorBackground)) // Une column Vide avant chaque caract\'E8re \'E0 ne pas supprimer\par
  961         \{\par
  962             isuivant=(++l)/8;\par
  963             ksuivant=(l)%8;\par
  964             nombreDecolumnVide=1;\par
  965 \par
  966             // compter les columns vide apr\'E8s la premi\'E8re afin de les supprimer\par
  967             // si plus de 4 c'est le caract\'E8re espace que l'on doit garder\par
  968 //          while(columnVide(ksuivant,chaine[isuivant],colorBackground) && nombreDecolumnVide++ < 6)\par
  969             while(EmptyColumn(ksuivant,chaine[isuivant],colorBackground) && nombreDecolumnVide++ < 6)\par
  970             \{\par
  971                 ImageContainment(l,chaine,taille);\par
  972                 nombreDecolumns--;\par
  973             \}\par
  974 \par
  975         \}\par
  976     \}\par
  977 \par
  978     // Parcours de toutes les columns de tous les patterns qui composent\par
  979     // la chaine de caract\'E8res \'E0 afficher (sans les columns vides superflues).\par
  980     for( l=0; l<nombreDecolumns; l++)\par
  981     \{\par
  982         // Decalage des columns vers la gauche sur l'image Numero 0 (celle qu'on affiche sur la matrice de LED\par
  983         for( i=0;i<taille;i++)\par
  984         \{\par
  985             // Cas Normal, les columns sont sur le m\'EAme pattern\par
  986             for(j=0;j<8;j++)\par
  987             \{\par
  988                 for(k=0;k<7;k++)\par
  989                     chaine[i][j][k]=chaine[i][j][k+1];\par
  990 \par
  991             \}\par
  992             // Cas o\'F9 l'on doit changer de pattern\par
  993             for(j=0;j<8;j++)\par
  994             \{\par
  995                 chaine[i][j][7]=chaine[i+1][j][0];\par
  996             \}\par
  997         \}\par
  998         usleep(1000*vitesseDefilement);\par
  999         ViewPattern(chaine[0]);\par
 1000     \}\par
 1001 \par
